{
  "plugins": ["prettier-plugin-organize-imports"],
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 70,
  "tabWidth": 2,
  "endOfLine": "crlf",
  "arrowParens": "always",
  "bracketSpacing": true,
  "experimentalTernaries": true,
  "importOrder": ["^components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
